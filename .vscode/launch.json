    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "showLog": true,
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/server",
            "env": {},
            "args": [],
            "substitutePath": [
                {
                    "from": "/home/shch/src",
                    "to": "/mnt/c/Users/shch/src",
                },
            ]
        }, {
            "type": "chrome",
            "request": "launch",
            "name": "vuejs: chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/client/src",
            "sourceMapPathOverrides": {
              "webpack:///src/*": "${webRoot}/*"
            }
          },
    ]
}